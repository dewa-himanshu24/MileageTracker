{
  "expo": {
    "name": "MileageTracker",
    "slug": "mileageTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "mileageTracker",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "rgba(245, 88, 88, 1)"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.himanshudewangan.mileageTracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "rgba(245, 88, 88, 1)"
      },
      "package": "com.himanshudewangan.mileageTracker"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bba22bf5-0103-4d38-ac58-a307a7d09322"
      }
    }
  }
}
